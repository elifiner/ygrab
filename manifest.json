{
    "manifest_version": 2,
    "name": "ygrab",
    "description": "Extract bank statements for YNAB",
    "homepage_url": "https://github.com/elifiner/ygrab",
    "version": "0.1",

    "permissions": [
        "activeTab"
    ],
    "icons": {
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon128.png",
        "default_popup": "popup.html"
    }
}