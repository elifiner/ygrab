{
    "manifest_version": 2,
    "name": "Autobank",  
    "description": "Extract bank statements",  
    "version": "1.0",    
    "permissions": ["activeTab"],  

    "browser_action": { 
        "default_icon": {
            "19": "images/icons/19.png",
            "38": "images/icons/38.png"
        },
        "default_popup": "popup.html"  
    },
    
    "icons": {
        "19": "images/icons/19.png",
        "38": "images/icons/38.png",
    }
}