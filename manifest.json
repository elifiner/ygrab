{
    "manifest_version": 2,
    "name": "Autobank",
    "description": "Extract bank statements for YNAB",
    "version": "1.0",
    "permissions": [
        "https://hb.unionbank.co.il/*",
        "file://*/ubi-account.html"
    ],
    "icons": {
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon128.png",
        "default_popup": "popup.html"
    }
}